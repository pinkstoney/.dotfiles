# BEGIN_KITTY_THEME
# Gruvbox Dark
include current-theme.conf
# END_KITTY_THEME
#
font_family Hack Nerd Font Mono
bold_font        auto
italic_font      auto
bold_italic_font auto
font_size 13.0

open_url_with default

copy_on_select yes
focus_follows_mouse yes

macos_traditional_fullscreen yes

cursor #fe7565
cursor_text_color background

tab_bar_style fade
